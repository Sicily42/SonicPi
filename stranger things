use_bpm 160

use_synth :prophet
live_loop :strange do
  play :c4
  sleep 0.5
  play :e4
  sleep 0.5
  play :g4
  sleep 0.5
  play :b4
  sleep 0.5
  play :c5
  sleep 0.5
  play :b4
  sleep 0.5
  play :g4
  sleep 0.5
  play :e4
  sleep 0.5
end
live_loop :heartbeat do
  sample :drum_heavy_kick
  sleep 3
  sample :drum_heavy_kick
  sleep 1
end

live_loop :laser do
  sample :ambi_drone
  sleep 1
  sample :ambi_haunted_hum
  sleep 3
end

