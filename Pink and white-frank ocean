use_synth :piano
use_bpm 160
define :hum do
  play :g4
  sleep 1.5
end
define :b1 do
  play :d3
  play :bb3
  play :g3
  sleep 1
  
end
define :b2 do
  play :c3
  play :a3
  play :f3
  sleep 1
  
end
define :b3 do
  play :b3
  play :g3
  play :e3
  sleep 1
  
end


sample bee
sleep 3.4

live_loop :background do
  6.times do
    b1
  end
  6.times do
    b2
  end
  9.times do
    b3
  end
end




